{
  "name": "rivets-utilify",
  "version": "0.1.0",
  "description": "Rivets formatters for using with UtilifyJS",
  "main": "dist/rivets-utilify.common.js",
  "module": "dist/rivets-utilify.esm.js",
  "browser": "dist/rivets-utilify.js",
  "scripts": {
    "build": "rollup -c",
    "clear": "del-cli dist",
    "compress": "uglifyjs dist/rivets-utilify.js -o dist/rivets-utilify.min.js -c -m --comments /^!!/",
    "lint": "eslint src --fix",
    "release": "npm run clear && npm run lint && npm run build && npm run compress",
    "start": "./node_modules/.bin/rollup -c --watch"
  },
  "repository": "zeindelf/rivets-utilify",
  "homepage": "https://github.com/zeindelf/rivets-utilify",
  "license": "MIT",
  "author": {
    "name": "Zeindelf",
    "email": "zeindelf@hotmail.com",
    "homepage": "https://www.zeindelf.com"
  },
  "files": [
    "dist",
    "doc",
    "lib"
  ],
  "keywords": [
    "vtex",
    "utils",
    "utilities",
    "helpers",
    "data",
    "masterdata",
    "rivets",
    "rivets-js",
    "rivets-utilify",
    "rivets-utilify.js",
    "javascript",
    "js-helpers",
    "js-utilities",
    "js-utils",
    "front-end",
    "web",
    "component",
    "development"
  ],
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.11.0",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^3.3.23"
  }
}
